{
  "name": "react-native-webview-comlink",
  "version": "0.5.1",
  "description": "",
  "main": "lib/web.bundle.js",
  "react-native": "lib/native.js",
  "module": "lib/web.js",
  "files": [
    "lib/"
  ],
  "scripts": {
    "clean": "rm -rf lib && rm -rf examples/web/node_modules/react-native-webview-comlink/lib && rm -rf examples/native/node_modules/react-native-webview-comlink/lib",
    "build": "npm run clean && npm run compile && npm run rollup-web",
    "postbuild": "cp -r lib examples/web/node_modules/react-native-webview-comlink/lib && cp -r lib examples/native/node_modules/react-native-webview-comlink/lib",
    "compile": "npm run compile-native && npm run compile-web",
    "compile-native": "tsc -p src/native --outDir lib -d",
    "compile-web": "tsc -p src/web --outDir lib -d",
    "rollup-web": "rollup -c rollup.config.web.js",
    "release": "npm run build && standard-version && git push --follow-tags origin master && npm publish",
    "test": "npm run build && jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rocwind/react-native-webview-comlink.git"
  },
  "keywords": [
    "comlink",
    "rpc",
    "react-native",
    "webview"
  ],
  "author": "Roc",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rocwind/react-native-webview-comlink/issues"
  },
  "homepage": "https://github.com/rocwind/react-native-webview-comlink#readme",
  "peerDependencies": {
    "comlinkjs": ">= 3.2.0"
  },
  "dependencies": {
    "hoist-non-react-statics": "^3.3.2",
    "message-port-polyfill": "^0.2.0",
    "wait-ready": "^0.5.1"
  },
  "devDependencies": {
    "@types/react": "17.0.2",
    "@types/react-native": "0.63.49",
    "comlinkjs": "3.2.0",
    "jest": "26.6.3",
    "metro-react-native-babel-preset": "0.65.1",
    "react": "16.11.0",
    "react-native": "0.63.4",
    "react-native-webview": "11.2.3",
    "react-test-renderer": "17.0.1",
    "rollup": "2.39.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "standard-version": "9.1.0",
    "typescript": "4.1.5"
  }
}
